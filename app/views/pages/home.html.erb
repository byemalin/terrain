        <!-- CENTER -->
        <section id="terrainCenter" class="terrainCard"></section>

        <!-- RIGHT -->
        <section id="terrainRight">

            <div id="dynamicControls" class="terrainCard">

                <div id="strokeWeightDiv">
                    <label for="strokeWeightRange">Stroke Weight</label>
                    <input type="number" id="strokeWeightRange" name="strokeWeightRange" min="0.1" max="50" step="0.1">
                </div>

                <!-- Terrain tool specific controls  -->
                <div id="terrainControls">
                    <label for="waveRadiusRange">Wave Radius</label>
                    <input type="range" id="waveRadiusRange" name="waveRadiusRange" min="1" max="1200">

                    <label for="waveStrengthRange">Wave Strength</label>
                    <input type="range" id="waveStrengthRange" name="waveStrengthRange" min="100" max="1000">

                    <label for="terrainSpeedRange">Speed</label>
                    <input type="range" id="terrainSpeedRange" name="terrainSpeedRange" min="1" max="5">
                </div>
            </div>


            <div id="toggleSwitches" class="terrainCard">
                <section id="fillToggles">
                    <div>
                        <label>Fill</label>
                        <label class="switch">
                            <input type="checkbox" id="fillSwitch" name="fillSwitch">
                            <span class="slider"></span>
                        </label>
                    </div>

                    <div>
                        <label>Random</label>
                        <label class="switch">
                            <input type="checkbox" id="randomFillSwitch" name="randomFillSwitch">
                            <span class="slider"></span>
                        </label>
                    </div>
                </section>

                <section id="strokeToggles">
                    <div>
                        <label>Stroke</label>
                        <label class="switch">
                            <input type="checkbox" id="strokeSwitch" name="strokeSwitch">
                            <span class="slider"></span>
                        </label>
                    </div>

                    <div>
                        <label>Random</label>
                        <label class="switch">
                            <input type="checkbox" id="randomStrokeSwitch" name="randomStrokeSwitch">
                            <span class="slider"></span>
                        </label>
                    </div>
                </section>
            </div>

